# Enable the Rewrite Engine
RewriteEngine On

# Set index-two as the default page
DirectoryIndex index-two

# Proxy all requests to the Next.js app running on port 3001
RewriteRule ^(.*)$ http://localhost:3000/$1 [P,L]

# Disable directory listing (optional)
Options -Indexes

# PHP Configuration (Generated by cPanel)
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php81 .php .php8 .phtml
</IfModule>